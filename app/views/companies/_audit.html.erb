<h2>Recent Activities</h2>
<table class="table">
  <thead>
    <th>Time</th>
    <th>Event Type </th>
    <th>Record Type</th>
    <th>Changes</th>
  </thead>
  <% @versions.reverse.each do |version| %>
    <tr>
      <td><%= version.created_at %></td>
      <td><%= version.event %></td>
      <td><%= version.item_type %></td>
      <td><%= version.reify&.changes || 'No data' %></td>
    </tr>
  <% end %>
</table>